class Solution(object):
    def arrayRankTransform(self, arr):
        """
        :type arr: List[int]
        :rtype: List[int]
        """
        if len(arr) == 0:
            return []
        arr2 = list(arr)
        arr2.sort()
        d = {}
        d[arr2[0]] = 1
        for i in range(1,len(arr2)):
            if arr2[i] not in d:
                d[arr2[i]] = d[arr2[i - 1]] + 1
        l = []
        for p in arr:
            l.append(d[p])
        return l

        
