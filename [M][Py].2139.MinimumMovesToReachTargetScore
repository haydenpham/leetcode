class Solution(object):
    def minMoves(self, target, maxDoubles):
        """
        :type target: int
        :type maxDoubles: int
        :rtype: int
        """
        c = 0
        while target > 1:
            if maxDoubles > 0:
                if target % 2 == 0:
                    target = target / 2
                    maxDoubles -= 1
                    c += 1
                else:
                    target -= 1
                    c += 1
            else:
                c += target - 1
                break
        return c
