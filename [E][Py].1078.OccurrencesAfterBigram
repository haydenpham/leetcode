class Solution(object):
    def findOcurrences(self, text, first, second):
        """
        :type text: str
        :type first: str
        :type second: str
        :rtype: List[str]
        """
        t = text.split(' ')
        l = []
        for i in range(len(t)):
            if t[i] == first and i + 2 < len(t) and t[i + 1] == second:
                l.append(t[i + 2])
        return l
