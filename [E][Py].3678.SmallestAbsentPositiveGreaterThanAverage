import math

class Solution(object):
    def smallestAbsent(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        average = sum(nums) / float(len(nums))
        nums_set = set(nums)
        m = max(nums)
        candidate = max(int(math.floor(average)) + 1, 1)
        
        while True:
            if candidate > 0 and candidate not in nums_set:
                return candidate
            candidate += 1
