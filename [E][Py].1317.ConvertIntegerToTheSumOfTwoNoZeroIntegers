class Solution(object):
    def getNoZeroIntegers(self, n):
        """
        :type n: int
        :rtype: List[int]
        """
        for i in range(1, n):
            if not self.isNoZeroInteger(i):
                continue
            if self.isNoZeroInteger(n-i):
                return [i, n-i]
        return []

    def isNoZeroInteger(self, n):
        s = str(n)
        return s.count('0') == 0
        
