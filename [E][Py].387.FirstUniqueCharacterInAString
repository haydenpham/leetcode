class Solution(object):
    def firstUniqChar(self, s):
        """
        :type s: str
        :rtype: int
        """
        d = {}
        for i in range(0, len(s)):
            if s[i] not in d:
                d[s[i]] = i
            else:
                d[s[i]] = -1
        minIndex = 100001
        for k in d:
            if d[k] > -1:
                minIndex = min(d[k], minIndex)
        if minIndex == 100001:
            return -1
        else:
            return minIndex
