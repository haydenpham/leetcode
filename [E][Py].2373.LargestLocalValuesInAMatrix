class Solution(object):
    def largestLocal(self, grid):
        """
        :type grid: List[List[int]]
        :rtype: List[List[int]]
        """
        rl = []
        maxVal = 0
        for i in range(1, len(grid) - 1):
            trl = []
            for j in range(1, len(grid[i]) - 1):
                maxVal = 0
                for t in range(i - 1, i + 2):
                    for k in range(j - 1, j + 2):
                        maxVal = max(maxVal, grid[t][k])
                trl.append(maxVal)
            rl.append(trl)
        return rl
