class Solution(object):
    def maximumEnergy(self, energy, k):
        """
        :type energy: List[int]
        :type k: int
        :rtype: int
        """
        n = len(energy)
        start_from = [0] * len(energy)
        i = n - 1
        while i >= n - k:
            start_from[i] = energy[i]
            i -= 1
        while i >= 0:
            start_from[i] = energy[i] + start_from[i + k]
            i -= 1
        return max(start_from)
